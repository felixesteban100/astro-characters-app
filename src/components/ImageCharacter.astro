---
import { Image } from "astro:assets";

// import Image from "astro/components/Image.astro";

interface Props {
  classes: string;
  id: string | number;
  src: string;
  height: number;
  width: number;
  alt: string;
  loading: "eager" | "lazy" | null | undefined;
}

const { classes, id, src, height, width, alt, loading } = Astro.props;
---

<!-- Your template here! -->
<Image
  class:list={[`${classes}`]}
  src={src}
  height={height}
  width={width}
  alt={alt}
  loading={loading}
  id={id.toString()}
/>
